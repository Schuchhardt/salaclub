{
  "name": "ensayapp-front",
  "version": "1.0.0",
  "description": "Ensayapp Front",
  "main": "index.js",
  "scripts": {
    "start": "DEV_ENV=1 webpack-dev-server --content-base app --hot --verbose",
    "build": "mkdir build && cp app/index.html build/index.html && NODE_ENV=production webpack --config webpack.production.config.js --verbose && cp -r ./app/images ./build",
    "staging": "mkdir build && cp app/index.html build/index.html && NODE_ENV=staging webpack --config webpack.production.config.js --verbose --progress && cp -r ./app/images ./build",
    "e2e": "protractor protractor.conf.js"
  },
